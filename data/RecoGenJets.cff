# $Id: RecoGenJets.cff,v 1.13 2007/10/26 22:33:17 fedor Exp $
#
# ShR 27 Mar 07: move modules producing candidates for Jets into separate cff file due to scheduling problem
#
include "RecoJets/Configuration/data/GenJetParticles.cff"

include "RecoJets/JetProducers/data/kt4GenJets.cff"
include "RecoJets/JetProducers/data/kt6GenJets.cff"
include "RecoJets/JetProducers/data/iterativeCone5GenJets.cff"
include "RecoJets/JetProducers/data/sisCone5GenJets.cff"
include "RecoJets/JetProducers/data/sisCone7GenJets.cff"

include "RecoJets/JetProducers/data/kt4GenJetsNoNuBSM.cff"
include "RecoJets/JetProducers/data/kt6GenJetsNoNuBSM.cff"
include "RecoJets/JetProducers/data/iterativeCone5GenJetsNoNuBSM.cff"
include "RecoJets/JetProducers/data/sisCone5GenJetsNoNuBSM.cff"
include "RecoJets/JetProducers/data/sisCone7GenJetsNoNuBSM.cff"

sequence jetsWithAll = {
       kt4GenJets & kt6GenJets & 
       iterativeCone5GenJets & 
       sisCone5GenJets & sisCone7GenJets
}

sequence jetsNoNuBSM = {
       kt4GenJetsNoNuBSM & kt6GenJetsNoNuBSM & 
       iterativeCone5GenJetsNoNuBSM & 
       sisCone5GenJetsNoNuBSM & sisCone7GenJetsNoNuBSM
}

sequence jetsWithAllPt10 = {
       kt4GenJetsPt10Seq & kt6GenJetsPt10Seq &
       iterativeCone5GenJetsPt10Seq & 
       sisCone5GenJetsPt10Seq & sisCone7GenJetsPt10Seq
}


# ShR 27 Mar 2007
# NB: genJetParticles sequence MUST be executed before this one but we cannot add it here
# since causes scheduling conflicts with modules from MET
sequence recoGenJets = {
    jetsWithAll & jetsNoNuBSM & jetsWithAllPt10
}
